public class GameObject {
    private Cell position; // Позиция клетки
    private char type; // Тип игрового объекта

    public GameObject(int x, int y, char type) {
        this.position = new Cell(x, y);
        this.type = type;
    }

    public char getType() {
        return type;
    }

    public Cell getPosition() {
        return position;
    }
}
